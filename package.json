{
  "name": "aceaspades-nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "cy:o": "cypress open",
    "cy:all": "cypress run && cypress run --component",
    "cy:e2e": "cypress run",
    "cy:c": "cypress run --component",
    "precommit": "yarn precommit:lint && yarn cy:c",
    "precommit:lint": "lint-staged && tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "pre-commit": "yarn run precommit"
  },
  "lint-staged": {
    "**/*.ts": "tsc-files --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@heroicons/react": "^2.0.13",
    "@tippyjs/react": "^4.2.6",
    "cookies": "^0.8.0",
    "jwt-decode": "^3.1.2",
    "keygrip": "^1.1.0",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@cypress/react": "^7.0.0",
    "@types/cookies": "^0.7.7",
    "@types/next": "^9.0.0",
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.11.0",
    "cypress-real-events": "^1.7.2",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "^2.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.4"
  }
}
